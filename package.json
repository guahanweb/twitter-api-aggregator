{
  "name": "open-source-monitor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "dev": "NODE_ENV=development LOG_LEVEL=verbose nodemon --watch './src/**' --ext 'ts,json' --exec 'ts-node src/server/index.ts'",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "rules": "ts-node src/rules.ts",
    "script": "ts-node scripts/index.ts",
    "serve": "ts-node src/server/index.ts",
    "start": "ts-node src/stream.ts",
    "start:prod": "npm run clean && npm run build && LOG_LEVEL=warn nodemon --exec \"node dist/stream\"",
    "start:redis": "docker run -p 6379:6379 -v /tmp/redis:/data -d redis redis-server --save 60 1 --loglevel warning",
    "report": "ts-node src/report.ts",
    "test": "jest",
    "ts-node": "ts-node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "commander": "^8.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "needle": "^3.0.0",
    "nodemon": "^2.0.15",
    "redis": "^4.0.6",
    "winston": "^3.7.2",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/needle": "^2.5.3",
    "@types/node": "^17.0.23",
    "@types/redis-mock": "^0.17.1",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "redis-mock": "^0.56.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
